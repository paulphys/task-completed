{
    "name": "obsidian-task-completion",
    "version": "0.12.0",
    "description": "Plays a sound everytime you complete a task",
    "main": "main.js",
    "scripts": {
        "dev": "esbuild main.ts --bundle --external:obsidian --outdir=.  --target=es2016 --format=cjs --sourcemap=inline --watch --loader:.mp3=dataurl",
        "build": "esbuild main.ts --bundle --external:obsidian --outdir=.  --target=es2016 --format=cjs --loader:.mp3=dataurl"
    },
    "keywords": [],
    "author": "@paulphys",
    "devDependencies": {
        "@types/node": "^16.11.1",
        "esbuild": "0.13.8",
        "obsidian": "^0.12.17",
        "tslib": "2.3.1",
        "typescript": "4.4.4"
    },
    "dependencies": {
        "play-sound": "^1.1.3",
        "react": "^17.0.2",
        "use-sound": "^4.0.1"
    }
}
